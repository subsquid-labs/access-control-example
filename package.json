{
  "name": "squid-evm-template",
  "private": true,
  "scripts": {
    "build": "rm -rf lib && tsc",
    "update": "npx npm-check-updates --filter /subsquid/ --upgrade && npm i -f",
    "db:migrate": "npx squid-typeorm-migration apply",
    "squid-etl:start": "node lib/processor.js",
    "squid-api:start": "squid-graphql-server"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@subsquid/big-decimal": "^1.0.0",
    "@subsquid/evm-processor": "^1.27.0",
    "@subsquid/evm-typegen": "4.3.0",
    "@subsquid/graphql-server": "4.9.0",
    "@subsquid/typeorm-migration": "1.3.0",
    "@subsquid/typeorm-store": "1.5.1",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.3",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@subsquid/typeorm-codegen": "2.0.2",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "typechain": "^8.3.2",
    "typescript": "~5.7.3"
  }
}
